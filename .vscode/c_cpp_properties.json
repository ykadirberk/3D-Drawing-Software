                "${workspaceFolder}/**",
                "${workspaceFolder}/SDK/OpenCV/include",
                "${workspaceFolder}/SDK/OpenCV/bin",
                "${workspaceFolder}/SDK/OpenCV/lib"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
